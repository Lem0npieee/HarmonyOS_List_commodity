import * as commonConst from '../common/CommonConstants';
import {
  SortOption,
  sortOptionMetas,
  filterOptionMetas,
  SubCategoryMeta,
  FilterId,
  SortOptionMeta,
  FilterOptionMeta
} from '../viewmodel/InitialData';
@Component
export default struct SelectedComponent {
  subCategories: SubCategoryMeta[] = [];
  currentSubCategory: string = '';
  searchValue: string = '';
  sortOption: SortOption = SortOption.Comprehensive;
  activeFilters: FilterId[] = [];
  sortOptionMetasData: SortOptionMeta[] = sortOptionMetas;
  filterOptionMetasData: FilterOptionMeta[] = filterOptionMetas;
  onSearchChange: (value: string) => void = () => {};
  onSubCategoryChange: (id: string) => void = () => {};
  onSortChange: (sort: SortOption) => void = () => {};
  onFilterToggle: (filter: FilterId) => void = () => {};

  private readonly searchPlaceholder: Resource = $r('app.string.search_placeholder');
  private readonly subCategoryScroller: Scroller = new Scroller();

  build() {
    Column() {
      this.renderSearchBar();
      this.renderSubCategories();
      this.renderSortOptions();
      this.renderFilterOptions();
      Divider().color($r('app.color.divider')).height(1);
    }
    .width(commonConst.LAYOUT_WIDTH_OR_HEIGHT)
    .padding({
      left: commonConst.GOODS_LIST_PADDING,
      right: commonConst.GOODS_LIST_PADDING,
      top: commonConst.GOODS_LIST_PADDING,
      bottom: commonConst.GOODS_LIST_PADDING
    })
  }

  @Builder
  private renderSearchBar() {
    Search({ value: this.searchValue, placeholder: this.searchPlaceholder })
      .searchButton('Go')
      .border({ width: 1, color: $r('app.color.divider') })
      .height(40)
      .width(commonConst.LAYOUT_WIDTH_OR_HEIGHT)
      .backgroundColor(Color.White)
      .onSubmit((value: string) => {
        this.onSearchChange(value);
      })
      .onChange((value: string) => {
        this.onSearchChange(value);
      })
  }

  @Builder
  private renderSubCategories() {
    Scroll(this.subCategoryScroller) {
      Row({ space: 8 }) {
        ForEach(this.subCategories, (sub: SubCategoryMeta) => {
          this.categoryChip(sub);
        }, (sub: SubCategoryMeta) => sub.id)
      }
      .padding({ top: 8, bottom: 8 })
    }
    .scrollable(ScrollDirection.Horizontal)
    .scrollBar(BarState.Off)
    .width(commonConst.LAYOUT_WIDTH_OR_HEIGHT)
  }

  @Builder
  private categoryChip(sub: SubCategoryMeta) {
    Text(sub.title)
      .padding({ left: 12, right: 12, top: 6, bottom: 6 })
      .fontSize(commonConst.GOODS_EVALUATE_FONT_SIZE)
      .backgroundColor(this.isSubCategoryActive(sub.id) ? $r('app.color.accent') : $r('app.color.softAccent'))
      .fontColor(this.isSubCategoryActive(sub.id) ? Color.White : $r('app.color.deepGray'))
      .borderRadius(16)
      .onClick(() => {
        this.onSubCategoryChange(sub.id);
      })
  }

  @Builder
  private renderSortOptions() {
    Column() {
      Text($r('app.string.sort_section_title'))
        .fontSize(commonConst.GOODS_EVALUATE_FONT_SIZE)
        .fontColor($r('app.color.deepGray'))
        .margin({ top: 6, bottom: 4 })
      Row({ space: 8 }) {
        ForEach(this.sortOptionMetasData, (option: SortOptionMeta) => {
          Text(option.title)
            .padding({ left: 12, right: 12, top: 6, bottom: 6 })
            .borderRadius(16)
            .backgroundColor(this.isSortOptionActive(option.id) ? $r('app.color.accent') : Color.Transparent)
            .border({
              width: 1,
              color: this.isSortOptionActive(option.id) ? $r('app.color.accent') : $r('app.color.divider')
            })
            .fontColor(this.isSortOptionActive(option.id) ? Color.White : $r('app.color.deepGray'))
            .fontSize(commonConst.GOODS_EVALUATE_FONT_SIZE)
            .onClick(() => {
              this.onSortChange(option.id);
            })
        }, (option: SortOptionMeta) => option.id)
      }
    }
  }

  @Builder
  private renderFilterOptions() {
    Column() {
      Text($r('app.string.filter_section_title'))
        .fontSize(commonConst.GOODS_EVALUATE_FONT_SIZE)
        .fontColor($r('app.color.deepGray'))
        .margin({ top: 12, bottom: 4 })
      Flex({ direction: FlexDirection.Row, wrap: FlexWrap.Wrap, justifyContent: FlexAlign.Start }) {
        ForEach(this.filterOptionMetasData, (filter: FilterOptionMeta) => {
          Text(filter.title)
            .padding({ left: 12, right: 12, top: 6, bottom: 6 })
            .borderRadius(16)
            .border({
              width: 1,
              color: this.isFilterActive(filter.id) ? $r('app.color.accent') : $r('app.color.divider')
            })
            .backgroundColor(this.isFilterActive(filter.id) ? $r('app.color.softAccent') : Color.Transparent)
            .fontColor(this.isFilterActive(filter.id) ? $r('app.color.freshRed') : $r('app.color.deepGray'))
            .fontSize(commonConst.GOODS_EVALUATE_FONT_SIZE)
            .margin({ right: 8, bottom: 8 })
            .onClick(() => {
              this.onFilterToggle(filter.id);
            })
        }, (filter: FilterOptionMeta) => filter.id)
      }
    }
  }

  private isSubCategoryActive(id: string): boolean {
    return this.currentSubCategory === id;
  }

  private isSortOptionActive(option: SortOption): boolean {
    return this.sortOption === option;
  }

  private isFilterActive(filter: FilterId): boolean {
    return this.activeFilters.indexOf(filter) >= 0;
